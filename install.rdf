<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>cs-en@dictionaries.addons.mozilla.org</em:id>
    <em:name>Slovník kombinující češtinu a angličtinu</em:name>
    <em:description>Přepínání jazyků ve Firefoxu je nepohodlné, toto je workaround</em:description>
    <!-- Combines:
    https://addons.mozilla.org/en-US/firefox/addon/czech-spell-checking-dictionar/
    https://addons.mozilla.org/en-US/firefox/addon/united-states-english-spellche/
    -->
    
    <em:version>1.0.0</em:version>
    <em:type>64</em:type>
    <em:unpack>true</em:unpack>

    <!--Firefox-->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>2.0b1</em:minVersion>
        <em:maxVersion>99.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!--Firefox pro Android-->
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>10.0a1</em:minVersion>
        <em:maxVersion>99.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
